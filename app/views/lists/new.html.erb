<h2>Create a new list</h2>
<%= form_for @list do |f| %>
        <%= f.label :title %>
        <%= f.text_field :title %><br><br>

        <%= f.submit "Create List"%>
<% end %>

<% if @list.errors.any? %>
        <% @list.errors.full_messages.each do |message| %>
        <%= message %>
        <% end %>
<% end %>
        