<h2>Create a new List</h2>
<%= form_for @list do |f| %>
        <%= f.label :title %>
        <%= f.text_field :title %><br><br>

        <%= button_to "Create task", new_task_path %><br>
        
        <%= f.label :start_time %>
        <%= f.time_field :start_time%>
        
        <%= f.label :start_date %>
        <%= f.date_field :start_date%><br>
        
        <%= f.label :end_date %>
        <%= f.date_field :end_date%>
        
        <%= f.label :end_time %>
        <%= f.time_field :end_time%><br><br>

        <%= f.submit "Create List"%>
<% end %>