<div class="fullCard"> 
    <div class="cardContent">
        <h1><%= @list.title %></h1> 

        <p><% @list.tasks.each do |task| %>
        <input type= "checkbox" >
        <strong><%= task.name %></strong><br>

            <span style="padding-left:2em"> 
            <%= task.description%>
            </span><br>

        </p>

        <% end %>

        <%= button_to "Create task", new_task_path, method: :get  %>
    </div>
</div>