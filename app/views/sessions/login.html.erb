<h1>Log Into Profile</h1>

<%= form_for @user, url: {controller: 'sessions', action: 'login_attempt'} do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit "Log Into Profile" %>
<% end %>

